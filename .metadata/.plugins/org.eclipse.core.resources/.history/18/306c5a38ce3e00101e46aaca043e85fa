<?xml version="1.0" encoding="UTF-8"?>
<project name="RSE">
	<property name="rse-core-classes"
	<target name="init">
		<mkdir dir="./bin/" />
	</target>

	<target name="clean">
		<delete dir="./bin/" />
	</target>

	<target name="compile" depends="init">
		<javac srcdir="./src/" destdir="./bin/" />
	</target>	
</project>